@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">service list</h1>
    <table class="table">
        <tr>
            <th>服务id</th>
            <th>服务名</th>
            <th>Ip</th>
            <th>端口</th>
            <th>状态</th>
        </tr>
           @{
        foreach(dynamic service in ViewBag.services)
        {
            <tr >
              <Td>
                @service.ServiceId
            </Td>
            <Td>
                @service.ServiceName
            </Td>
            <Td>
                @service.Ip
            </Td>
            <Td>
                @service.Port
            </Td>
             <Td>
                @service.Status
            </Td>
            </tr>
        }
    }
    </table>

    <p>
       _ds.RandomOne("user_center").AsHttpHost(): @ViewBag.userCenterBaseUrl
    </p>
 
</div>
