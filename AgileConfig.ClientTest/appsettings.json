{
  "AgileConfig": {
    "appId": "test_app",
    "secret": "test_app",
    //"nodes": "http://agileconfig_server.xbaby.xyz/",
    "nodes": "http://localhost:5000",
    "name": "哈哈",
    "tag": "tag1",
    "env": "DEV",
    "httpTimeout": "10", //s
    "cache": { // 指定缓存文件的目录
      "directory": "c:/data"
    },
    "serviceRegister": { //服务注册信息
      "serviceId": "test_app_service_02", //服务id，全局唯一，用来唯一标示某个服务
      "serviceName": "test_client", //服务名，可以重复，某个服务多实例部署的时候这个serviceName就可以重复
      "ip": "127.0.0.1", //服务的ip
      "port": 5002, //服务的端口
      "alarmUrl": "http://127.0.0.1:5100/home/servicedown", //告警地址，当服务不健康或者被移除的时候会往这个url post 数据，以便提醒
      "metaData": [
        "this is a test client"
      ],
      "heartbeat": {
        "interval": 5
      }
    }
  }
}